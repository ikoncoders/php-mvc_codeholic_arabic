{
    "name": "ikonc/php-mvc",
    "description": "A simple PHP framework for building web application",
    "keywords": ["framework", "php", "php-core","php-mvc"],
    "type": "library",
    "license": "MIT",
    "readme": "",
    "time": "",
    "support":{
        "issues": "https://github.com/TheCodingWorkshop/MagmaCore/issues",
        "source": "https://github.com/TheCodingWorkshop/MagmaCore",
        "wiki": "https://github.com/TheCodingWorkshop/MagmaCore/wiki"
    },
    "authors": [
        {
            "name": "ikoncoders",
            "email": "ikoncoders@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "App",
            "Ikonc\\PhpMvc\\": "src/"
        },
        "classmap": [
            "src/Model"
        ]
    },
    "autoload-dev":{
        "psr-4":{
            "Ikonc\\PhpMvc\\Tests\\": "tests/"
        }
    },
  
    "minimum-stability": "dev",
    "require": {
        "vlucas/phpdotenv": "^5.5",
        "symfony/var-dumper": "^6.3",
        "ext-mbstring": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "twig/twig": "^3.1",
        "symfony/http-foundation": "^5.2@RC",
        "symfony/yaml": "^5.2@RC",
        "illuminate/database": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
    
}
